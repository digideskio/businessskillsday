- title 'Students'

- content_for :sidebar do
  = render :partial => 'admin/sidebar'

- if notice.present?
  %p#notice.alert.alert-success= notice

- content_for :section_header do
  %h1= icon_title_student 'Students'

= render :partial => 'index', :locals => { :students => @admin_students, :section_header => content_for(:section_header), :show_all => true }