- title 'Accounts'

- content_for :sidebar do
  = render :partial => 'admin/sidebar'

- if notice.present?
  %p#notice.alert.alert-success= notice

- content_for :section_header do
  %h1= icon_title_account 'Accounts'

= render :partial => 'index', :locals => { :accounts => @admin_accounts, :section_header => content_for(:section_header), :show_all => true }